{
  "name": "tourze/yii2-helpers",
  "description": "Split helpers directory from yii2 framework",
  "type": "library",
  "require": {
    "php": "^8.1",
    "tourze/yii2-base": "0.0.*",
    "ezyang/htmlpurifier": "^4.17",
    "cebe/markdown": "~1.0.0 | ~1.1.0 | ~1.2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^2.1",
    "maglnet/composer-require-checker": "^4"
  },
  "license": "BSD-3-Clause",
  "autoload": {
    "psr-4": {
      "yii\\helpers\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "yii\\helpers\\tests\\": "tests/"
    }
  }
}